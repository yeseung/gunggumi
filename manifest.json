{
    "manifest_version": 3,
    "name": "gunggumi",
    "version": "1.0",
    "description": "궁구미 관련 웹사이트",
    "icons": {
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_title": "Click to open panel"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "permissions": ["sidePanel"]
}
